<%= form_for Bookmark.new do |f| %>

  <div class ="field">
    <%= f.label :url %><br>
    <%= f.text_field :url %>
  </div>

  <div class ="field">
    <%= f.label :description %><br>
    <%= f.text_field :description %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>

<% end %>

<% @bookmarks.each do |bookmark| %>
  <p><%= bookmark.url %></p>
  <p><%= bookmark.description %></p>
<% end %>
