 <div>
    <h5 class="center-align">Make a new list</h5>
  </div>
        
  <%= form_for List.new do |f| %>

    <div class="input-field">
      <%= f.label :name %><br>
      <%= f.text_field :name %>
    </div>

  <div class="actions">
    <%= f.submit %>
  </div>
  <% end %>

  <div class="col s9">
  <% @bookmarks.each do |bookmark| %>

  <a href="<%= bookmark.url %>" target="_blank">
<div class="card">
<div class="card-image">
  <img src="<%= bookmark.image %>">
  <span class="card-title #000000 black"><%= bookmark.title %></span>
</div>
<div class="card-content">
  <p><%= bookmark.summary %></p>
</div>
<div class="card-action">
  <a href="<%= bookmark.url %>" target="_blank">Posted by: <%= bookmark.user.first_Name %> at <i><%= bookmark.created_at.strftime("%I:%M%p %d %b %Y") %></a></i></a>

</div>
</div>
<% end %>
</div>
</div>

